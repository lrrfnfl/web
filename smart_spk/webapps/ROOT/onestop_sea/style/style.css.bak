/*---------------------- user-id-container s */
.layer
{position:fixed; 
top:0; 
left:0; 
width:100%; 
height:100%; 
background:rgba(0,0,0,0.8); 
z-index:1; 
cursor:pointer; }

.user-id-container
{position:fixed; 
top:350px; 
/* right:-600px;  */
right:-650px; 
width:460px; 
border:1px solid #333;
border-radius:8px; 
background:#fff; 
z-index:2; 
-webkit-box-sizing: border-box;
-moz-box-sizing: border-box;
box-sizing: border-box;
transition:0.3s; 
box-shadow:2px 2px 16px 4px rgba(0,0,0,0.6); }

.user-id-container.active
{right:-1px; }

.user-id-container-inner
{position:relative; }

.user-id-close
{position:absolute; 
top:0; 
left:-75px;
width:60px; 
height:100%; 
text-align:center;
display:-webkit-box;
display:-ms-flexbox;
display:-webkit-flex;
display:flex;
-webkit-box-pack:center;
-ms-flex-pack:center;
-webkit-justify-content:center;
justify-content:center;
-webkit-box-align:center;
-ms-flex-align:center;
-webkit-align-items:center;
align-items:center;
overflow:hidden; }

.user-id-close a
{width:50px; 
height:50px; 
display:inline-block; 
border-radius:50%;
text-align:center; 
margin-right:10px; 
background:rgba(221, 8, 49, 0.6);}

.user-id-close a img
{width:50%; 
margin-top:13px; }

.id-search-title
{font-size:17px; 
color:#fff;
padding:11px 15px; 
-webkit-box-sizing: border-box;
-moz-box-sizing: border-box;
box-sizing: border-box;
background: #e8e8e8;
background: -webkit-linear-gradient(#404040, #242424); 
background: -o-linear-gradient(#404040, #242424); 
background: -moz-linear-gradient(#404040, #242424);
background: linear-gradient(#404040, #242424); 
font-weight:600; 
border-bottom:1px solid #8e8e8e;}

.id-search-container
{padding:30px 15px; 
-webkit-box-sizing: border-box;
-moz-box-sizing: border-box;
box-sizing: border-box;}

.id-search-info
{margin-bottom:10px; 
line-height:1.4; }

.id-search-info p
{color:#1077a1;
font-size:13px; }

.id-search-wrap
{margin-bottom:15px; }

.id-search-wrap span
{font-weight:600; }

.id-search-wrap label input
{padding:3px; 
border-radius:1px; 
border:1px solid #cccccc;}

.search a
{display:inline-block; 
padding:4px 15px;
background:#dd0831;
color:#fff; 
border-radius:1px; }

.result-area
{display:inline-block; 
border:1px solid #125273;
background:#267199; 
color:#fff; 
padding:3px 12px; 
margin-left:20px; 
border-radius:1px; }

.id-search-table table
{border:1px solid #000;
width:100%; }

.id-search-table table th
{padding:5px 0; 
background:#000; 
color:#fff; }

.id-search-table table td
{padding:5px 0; 
text-align:center; 
border-bottom:1px solid #eee; 
word-break:break-all; }

.id-search-table table tr:last-child td
{border-bottom:0 none; }

.id-search-table table tr:hover td
{background:#eee; 
cursor:pointer; }

.paging
{text-align:center; 
margin-top:15px; }

.paging a
{display:inline-block; 
margin:0 1px; 
padding:3px 5px; 
font-size:15px; }

.paging a:hover
{font-weight:600; }

.paging .present a
{color:#f68e3e;
font-weight:bold; }
/*---------------------- user-id-container e */

/*---------------------- error-page s */
.error-page
{width:100%; 
height:100%; 
background:#f8f8f8; 
text-align:center; 
overflow:hidden; }

.error-page-inner
{padding-top:300px; }

.error-page-inner p
{color:#777777; 
font-size:20px; 
margin-bottom:5px; }

.error-header img
{display:inline-block; 
margin-bottom:50px; }
/*---------------------- error-page e */


.technical-link a
{float:right;  
font-size:19px;
color:#235592; }

.technical-link a:hover
{text-decoration:underline; }


/* ----------------------------- footer s */
.footer-wrap
{width:100%; 
border-top:4px solid #616161; 
background:#f8f8f8;}

.footer
{width:80%;
min-width:820px; 
max-width:1100px; 
margin:0 auto;}

.footer-inner
{display:inline-block; 
width:100%; 
padding:30px 0 20px 0; }

.footer-left
{float:left; 
width:15%; 
