
*, body
{margin:0; 
padding:0; 
font-size:13px; 
color:#333; 
font-family:"나눔고딕", NanumGothic;}

img
{border:0 none; }

a
{text-decoration:none; 
color:#333; }

ul, ol
{list-style-type:none; }

table
{border-collapse:collapse; }

.wrap-inner
{width:100%; }

.wrap-inner h1
{padding:22px 0; 
text-align:Center; 
margin-bottom:20px; }

/* header s */
.header
{width:100%; 
background: #e8e8e8;
background: -webkit-linear-gradient(#f9f9f9, #e7e7e6); 
background: -o-linear-gradient(#f9f9f9, #e7e7e6); 
background: -moz-linear-gradient(#f9f9f9, #e7e7e6);
background: linear-gradient(#f9f9f9, #e7e7e6); 
padding:15px 0 10px 0; 
border-bottom:3px solid #acacac;}

.header-inner
{width:80%;
min-width:820px; 
max-width:1100px; 
margin:0 auto; 
position:relative; }

.header-inner a img
{width:135px;
display:inline-block; 
margin-right:8px; }

.header-inner span img
{width:215px;}

.position-right
{position:absolute; 
bottom:0; 
right:0; }

.header-inner span.position-right img
{width:145px;}
/* header e */


.container
{width:700px; 
margin:0 auto; }

.block-title
{font-size:16px;
margin-bottom:10px; 
color:#dd0831;
font-weight:600; }


/*---------------------- corp-info s */
.corp-info
{width:100%;
margin:33px 0 50px 0; }

.corp-table
{display:inline-block; 
width:100%; 
border:1px solid #8e8e8e;
border-radius:4px; 
overflow:hidden; }

.corp-row
{display:inline-block; 
width:100%; }

.corp-row > div
{float:left; 
display:inline-block; 
width:50%; 
text-align:center; 
padding:10px 0; 
border-right:1px solid #8e8e8e; 
-webkit-box-sizing: border-box;
-moz-box-sizing: border-box;
box-sizing: border-box;}

.corp-row > div:last-child
{border-right:0 none; }

.corp-row.corp-title
{background: #e8e8e8;
background: -webkit-linear-gradient(#404040, #242424); 
background: -o-linear-gradient(#404040, #242424); 
background: -moz-linear-gradient(#404040, #242424);
background: linear-gradient(#404040, #242424); 
font-weight:600; 
border-bottom:1px solid #8e8e8e; }

.corp-row.corp-title > div
{color:#fff; }
/*---------------------- corp-info e */

.download
{width:100%; }

.block-title
{font-size:}

.download img
{width:100%; }

/*---------------------- guide-wrap s */
.guide-wrap
{width:100%;
margin:50px auto 0; }

.guide-block
{border:1px solid #8e8e8e;
border-radius:8px;
width:100%; 
margin-bottom:25px; 
position:relative; 
-webkit-box-sizing: border-box;
-moz-box-sizing: border-box;
box-sizing: border-box;
display:inline-block; 
overflow:hidden; }

.guide-instruction 
{display:inline-block; 
-webkit-box-sizing: border-box;
-moz-box-sizing: border-box;
box-sizing: border-box;
background: #cbcbcb;
background: -webkit-linear-gradient(#e4e4e4, #cbcbcb); 
background: -o-linear-gradient(#e4e4e4, #cbcbcb); 
background: -moz-linear-gradient(#e4e4e4, #cbcbcb);
background: linear-gradient(#e4e4e4, #cbcbcb); 
width:100%; 
padding:8px 2%; 
border-bottom:1px solid #b7b7b7;}

.numbering
{float:left; 
display:inline-block; 
width:32px; 
height:32px; 
border-radius:50%; 
text-align:center; 
background:#5a5a5a; }

.numbering span
{display:inline-block; 
color:#fff; 
font-size:20px;
margin-top:4px; }

.guide-text p
{font-size:17px; 
float:right; 
width:93%; 
margin:5px 0 0 0; 
line-height:1.4; 
color:#353535; 
font-weight:600; }

.guide-text p b
{font-size:17px; 
font-weight:700; }

.guide-text p .corp-id-name
{font-size:19px; 
color:#e31936}

.guide-img
{padding:30px 8%; 
